{
  "name": "node-mongo-template",
  "type": "module",
  "version": "0.1.0",
  "private": true,
  "packageManager": "pnpm@8.15.1",
  "description": "A template for a Node.js and MongoDB backend",
  "author": "Bryce",
  "keywords": [
    "express"
  ],
  "scripts": {
    "start": "esno ./src/index.ts",
    "dev": "esno watch ./src/index.ts",
    "prod": "node index.js",
    "build": "pnpm clean && tsup",
    "clean": "rimraf build",
    "lint": "eslint .",
    "lint:fix": "eslint . --fix",
    "prisma-migrate": "npx prisma migrate dev --name init",
    "prisma-deploy": "npx prisma migrate deploy"
  },
  "dependencies": {
    "@bryce-loskie/utils": "^0.3.4",
    "@prisma/client": "5.9.1",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "dotenv": "^16.4.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "log4js": "^6.9.1",
    "morgan": "^1.10.0",
    "prisma": "^5.9.1",
    "sqlite": "^5.1.1"
  },
  "devDependencies": {
    "@antfu/eslint-config": "^2.6.4",
    "@types/bcrypt": "^5.0.2",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/morgan": "^1.9.9",
    "@types/node": "^20.11.16",
    "eslint": "^8.56.0",
    "esno": "^4.0.0",
    "prettier": "^3.2.5",
    "rimraf": "^5.0.5",
    "tsup": "^8.0.1",
    "typescript": "^5.3.3"
  }
}
